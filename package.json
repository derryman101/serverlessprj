{
  "name": "serverless-api",
  "version": "1.0.0",
  "description": "Serverless API with Lambda and API Gateway",
  "main": "lambda_function.py",
  "scripts": {
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "deploy:staging": "serverless deploy --stage staging",
    "logs:dev": "serverless logs -f api --stage dev --tail",
    "logs:prod": "serverless logs -f api --stage prod --tail",
    "remove:dev": "serverless remove --stage dev",
    "info": "serverless info",
    "test": "pytest tests/ -v"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "serverless": "^3.40.0",
    "serverless-python-requirements": "^6.1.0"
  }
}